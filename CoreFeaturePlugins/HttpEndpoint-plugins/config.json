{
  "featureName": "http-endpoint-plugins",
  "afterFeatures": ["http-endpoint"],
  "objects": [
    {
      "kind": "raw",
      "dependency": "info.smart_tools.smartactors.message_processing.chain_call_receiver.ChainCallReceiver",
      "strategyDependency": "chain choice strategy",
      "name": "chain_selector"
    },
    {
      "kind": "stateless_actor",
      "dependency": "HttpRequestSenderActor",
      "name": "request-sender"
    }
  ],
  "maps": [
    {
      "id": "SelectChain",
      "steps": [
        {
          "target": "chain_selector"
        }
      ],
      "exceptional": [
      ]
    },
    {
      "id": "sendRequest",
      "steps": [
        {
          "target": "request-sender",
          "handler": "sendRequest",
          "wrapper": {
            "in_getRequest": "message/request"
          }
        }
      ],
      "exceptional": []
    }
  ],
  "endpoints": [
    {
      "name": "mainHttpEp",
      "type": "http",
      "port": 9909,
      "startChain": "SelectChain",
      "maxContentLength": 4098,
      "stackDepth": 5
    }
  ]
}
