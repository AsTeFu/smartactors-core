{
  "featureName": "info.smart_tools.smartactors:database-postgresql-async-ops-collection",
  "afterFeatures": [
    "info.smart_tools.smartactors:database-postgresql",
    "info.smart_tools.smartactors:database-plugins",
    "info.smart_tools.smartactors:database-postgresql-plugins"
  ],
  "objects": [
    {
      "kind": "stateless_actor",
      "dependency": "PostgresAsyncOpsCollectionActor",
      "name": "PostgresAsyncOpsCollection"
    }
  ],
  "maps": [
    {
      "id": "registerPostgresAsyncOpsCollection",
      "externalAccess": false,
      "steps": [
        {
          "target": "PostgresAsyncOpsCollection",
          "handler": "register",
          "wrapper": {
            "in_getConnectionOptionsRegistrationName": "message/connectionOptionsRegistrationName",
            "in_getCollectionName": "message/collectionName"
          }
        }
      ],
      "exceptional": []
    }
  ]
}
